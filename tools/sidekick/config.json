{
  "project": "Franklin Assets Selector",
  "plugins": [
    {
      "id": "aemedit",
      "title": "Edit",
      "environments": ["dev", "preview", "live"],
      "event": "aemedit"
    },
    {
      "id": "asset-library",
      "title": "My Assets",
      "environments": ["edit"],
      "url": "https://experience.adobe.com/solutions/CQ-helix-assets-addon/static-assets/resources/asset-selector.html",
      "isPalette": true,
      "includePaths": ["**.docx**", "**/docs.google.com/**"],
      "passConfig": true,
      "passReferrer": true,
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:400px; height: calc(100vh - 60px)"
    },
    {
      "id": "library",
      "title": "Library",
      "environments": ["edit", "preview"],
      "url": "/tools/sidekick/library.html",
      "includePaths": ["**.docx**", "**/docs.google.com/**"]
    }
  ]
}
